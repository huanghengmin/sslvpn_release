<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="balancer" name="com.hzih.sslvpn.domain.Balancer">
        <id access="field" name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="host" column="host" access="field" type="java.lang.String"/>
        <property name="port" column="port" access="field" type="int"/>
    </class>
</hibernate-mapping>
