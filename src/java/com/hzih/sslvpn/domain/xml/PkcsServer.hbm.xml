<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="pkcs" name="com.hzih.sslvpn.domain.PkcsServer">
        <id access="field" name="id" type="int" column="id" length="10">
            <!--<generator class="increment"/>-->
        </id>
        <property name="pkcs_crt" length="50" column="pkcs_crt" access="field" type="string"/>
        <property name="pkcs_name" length="50" column="pkcs_name" access="field" type="string"/>
        <property name="cn" length="50" column="cn" access="field" type="string"/>
        <property name="notAfter" length="50" column="notbefore" access="field" type="string"/>
        <property name="notBefore" length="50" column="notafter" access="field" type="string"/>
    </class>
</hibernate-mapping>
